{
    "cli": {
        "version": ">= 3.15.1"
    },
    "build": {
        "preview": {
            "env": {
                "EXPO_IMAGE_UTILS_CACHE_DIR": "/tmp/expo-image-cache",
                "EXPO_WEB_CACHE_DIR": "/tmp/expo-web-cache",
                "EXPO_HOME": "/tmp/expo"
            },
            "android": { "buildType": "apk" }
        },
        "preview:device": {
            "env": {
                "EXPO_IMAGE_UTILS_CACHE_DIR": "/tmp/expo-image-cache",
                "EXPO_WEB_CACHE_DIR": "/tmp/expo-web-cache",
                "EXPO_HOME": "/tmp/expo"
            }
        },
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_IMAGE_UTILS_CACHE_DIR": "/tmp/expo-image-cache",
                "EXPO_WEB_CACHE_DIR": "/tmp/expo-web-cache",
                "EXPO_HOME": "/tmp/expo"
            },
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "env": {
                "EXPO_IMAGE_UTILS_CACHE_DIR": "/tmp/expo-image-cache",
                "EXPO_WEB_CACHE_DIR": "/tmp/expo-web-cache",
                "EXPO_HOME": "/tmp/expo"
            }
        }
    },
    "submit": {
        "production": {}
    }
}